{
  "name": "elysia-bun-dler",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "type-check": "bun run tsc --noEmit",
    "build:exe": "bun run build.ts --compile --outfile server.exe",
    "db:migrate": "bun run drizzle-kit migrate",
    "db:generate": "bun run drizzle-kit generate",
    "db:push": "bun --bun drizzle-kit push"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.4.5",
    "@elysiajs/openapi": "^1.4.13",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@sinclair/typebox": "^0.34.47",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^1.0.0-beta.10-42c284e",
    "drizzle-typebox": "^1.0.0-beta.10-42c284e",
    "elysia": "latest",
    "elysia-http-problem-json": "^0.1.4",
    "exceljs": "^4.4.0",
    "imap-simple": "^5.1.0",
    "immer": "^11.0.1",
    "logixlysia": "^6.1.0",
    "lucide-react": "^0.562.0",
    "react": "^19",
    "react-dom": "^19",
    "tailwind-merge": "^3.3.1",
    "ultracite": "6.3.10",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/bun": "latest",
    "@types/imap-simple": "^4.2.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "bun-types": "latest",
    "drizzle-kit": "^1.0.0-beta.10-42c284e",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.4.0"
  }
}
